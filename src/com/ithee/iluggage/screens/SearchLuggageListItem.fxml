<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox onMouseClicked="#onClick" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ithee.iluggage.screens.SearchLuggageListItem">
    <children>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints />
                <ColumnConstraints hgrow="SOMETIMES" prefWidth="80.0" />
                <ColumnConstraints />
                <ColumnConstraints hgrow="SOMETIMES" prefWidth="80.0" />
                <ColumnConstraints />
                <ColumnConstraints hgrow="SOMETIMES" prefWidth="80.0" />
                <ColumnConstraints />
                <ColumnConstraints hgrow="SOMETIMES" prefWidth="80.0" />
                <ColumnConstraints />
                <ColumnConstraints hgrow="SOMETIMES" prefWidth="80.0" />
                <ColumnConstraints hgrow="SOMETIMES" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints vgrow="ALWAYS" />
                <RowConstraints minHeight="24.0" valignment="BOTTOM" vgrow="ALWAYS" />
                <RowConstraints vgrow="ALWAYS" />
            </rowConstraints>
            <children>
                <Text text="%kind" GridPane.columnIndex="0">
                    <GridPane.margin>
                        <Insets right="5.0" />
                    </GridPane.margin>
                </Text>
                <Text text="%color" GridPane.columnIndex="2">
                    <GridPane.margin>
                        <Insets left="10.0" right="5.0" />
                    </GridPane.margin>
                </Text>
                <Text text="%brand" GridPane.columnIndex="4">
                    <GridPane.margin>
                        <Insets left="10.0" right="5.0" />
                    </GridPane.margin>
                </Text>
                <Text text="%size" GridPane.columnIndex="6">
                    <GridPane.margin>
                        <Insets left="10.0" right="5.0" />
                    </GridPane.margin>
                </Text>
                <Text text="%stickers" GridPane.columnIndex="8">
                    <GridPane.margin>
                        <Insets left="10.0" right="5.0" />
                    </GridPane.margin>
                </Text>
            
                <Text fx:id="txtKind" underline="true" GridPane.columnIndex="1" />
                <Text fx:id="txtColor" underline="true" GridPane.columnIndex="3" />
                <Text fx:id="txtBrand" underline="true" GridPane.columnIndex="5" />
                <Text fx:id="txtSize" underline="true" GridPane.columnIndex="7" />
                <Text fx:id="txtStickers" underline="true" GridPane.columnIndex="9" />
                <Text text="%miscellaneous" GridPane.columnSpan="10" GridPane.rowIndex="1">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Text>
                <Text fx:id="txtMisc" GridPane.columnSpan="10" GridPane.rowIndex="2" />
            
                <Button onAction="#onClickDelete" GridPane.columnIndex="10">
                    <graphic>
                        <ImageView fitHeight="20" fitWidth="20">
                            <image>
                                <Image url="@../../../../trash.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children>
        </GridPane>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <cursor>
        <Cursor fx:constant="HAND" />
    </cursor>
</VBox>
