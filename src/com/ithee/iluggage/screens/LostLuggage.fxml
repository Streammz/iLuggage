<BorderPane prefHeight="560.0" styleClass="root" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ithee.iluggage.screens.LostLuggage">
    <stylesheets>
        <URL value="@../../../../main.css" />
    </stylesheets>
   <center>
       
      <GridPane hgap="5" vgap="5" BorderPane.alignment="CENTER">
          <BorderPane.margin><Insets bottom="10.0" left="10.0" right="10.0" top="10.0" /></BorderPane.margin>
        <columnConstraints>
          <ColumnConstraints hgrow="NEVER" prefWidth="100.0" />
          <ColumnConstraints hgrow="ALWAYS" minWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
          <RowConstraints prefHeight="30.0" vgrow="NEVER" />
          <RowConstraints prefHeight="30.0" vgrow="NEVER" />
          <RowConstraints prefHeight="30.0" vgrow="NEVER" />
          <RowConstraints prefHeight="30.0" vgrow="NEVER" />
          <RowConstraints prefHeight="30.0" vgrow="NEVER" />
          <RowConstraints prefHeight="30.0" vgrow="NEVER" />
          <RowConstraints prefHeight="50.0" vgrow="ALWAYS" />
          <RowConstraints prefHeight="30.0" vgrow="NEVER" />
        </rowConstraints>
         <children>
            <Text text="Soort" textAlignment="RIGHT" wrappingWidth="100" GridPane.rowIndex="4" />
            <Text text="Merk" textAlignment="RIGHT" wrappingWidth="100" GridPane.rowIndex="5" />
            <Text text="Kleur" textAlignment="RIGHT" wrappingWidth="100" GridPane.rowIndex="6" />
            <Text text="Formaat" textAlignment="RIGHT" wrappingWidth="100" GridPane.rowIndex="7" />
            <Text text="Stickers" textAlignment="RIGHT" wrappingWidth="100" GridPane.rowIndex="8" />
            <Text text="Vluchtnummer" textAlignment="RIGHT" wrappingWidth="100" GridPane.rowIndex="9" />
            <Text text="Overige Kenmerken" textAlignment="RIGHT" wrappingWidth="100" GridPane.rowIndex="10" />
            
            <ChoiceBox fx:id="chKind" maxWidth="Infinity" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <AutocompleteTextField fx:id="tfBrand" maxWidth="Infinity" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <AutocompleteTextField fx:id="tfColor" maxWidth="Infinity" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <HBox spacing="5" GridPane.columnIndex="1" GridPane.rowIndex="7">
                <children>
                    <TextField fx:id="tfSize1" HBox.hgrow="ALWAYS" />
                    <TextField fx:id="tfSize2" HBox.hgrow="ALWAYS" />
                  <TextField fx:id="tfSize3" layoutX="204.0" layoutY="10.0" />
                </children>
            </HBox>
            <CheckBox fx:id="cbStickers" maxWidth="Infinity" GridPane.columnIndex="1" GridPane.rowIndex="8" />
            <TextField fx:id="tfFlightnr" maxWidth="Infinity" GridPane.columnIndex="1" GridPane.rowIndex="9" />   
            <TextArea fx:id="tfMisc" maxWidth="Infinity" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="10" />
            
            <HBox alignment="CENTER_RIGHT" spacing="5" GridPane.columnSpan="2" GridPane.rowIndex="11">
                <children>
                    <Button onAction="#onCancel" text="Annuleer" />
                    <Button onAction="#onAdd" text="Toevoegen" />
                </children>
            </HBox>
            <Separator prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="3" />
            <Text text="Telefoonnummer:" textAlignment="RIGHT" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
            <TextField fx:id="tfPhone" prefHeight="31.0" prefWidth="303.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Text text="E-mail:" textAlignment="RIGHT" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
            <TextField fx:id="tfEmail" prefHeight="31.0" prefWidth="196.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Text text="Naam klant:" textAlignment="RIGHT" GridPane.halignment="RIGHT" />
              <TextField fx:id="tfCustomername" GridPane.columnIndex="1" /> 
         </children>
      </GridPane>
   </center>
</BorderPane>
